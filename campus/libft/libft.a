void	*ft_memcpy(void *dest, const void *srcm size_t n)
{
	size_t i;

	i = 0;
	if (!dest && !src)
		return (NULL);
	if (dest != src)
	{
		while (i < n)
		{
			((unsigned char *)dest[i] = (unsigned char *)src[i])
			i++;
		}
	}
	return (dest);
}
